{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "max-line-length": [true, 180],
        "indent": [true, "tabs", 1],
        "array-type": [false],
        "member-access": false,
        "no-angle-bracket-type-assertion": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ]
    },
    "rulesDirectory": []
}
